  "paths": {
    "/rest/v2/projects": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Get a user's projects",
        "description": "Get the list of a user's projects together with their id and name",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "schema": {
              "type": "string"
            },
            "example": "Bearer 584d48fdb02a2186b7ed7455dca6d23332098ae7"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      },
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Adding a new project",
        "description": " Create a new project inside the workspace. It returns the id of the new project.",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Hiking"
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "schema": {
              "type": "string"
            },
            "example": "Bearer 584d48fdb02a2186b7ed7455dca6d23332098ae7"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/rest/v2/tasks": {
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Adding a new task",
        "description": "Create a new task inside a project based on the project id. It returns the id of the new task.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "content": "Buy GPUs",
                  "project_id": "2335467740"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
