      "post": {
        "tags": [
          "default"
        ],
        "summary": "Updating a task",
        "description": "Update the due date of a task. It returns the id of the task.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "due_string": "tomorrow"
                }
