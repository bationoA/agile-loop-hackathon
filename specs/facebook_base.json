        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "prompt": "You are useful content generator. You always generate a text based on user's request.",
                  "message": "Generate a text about climate change."
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {"type": "object"},
                "examples": {
                  "response": {
                    "value": {"generated_text": "{{generated_text}}"}

                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/images/generations": {
      "generateimage": {
        "tags": [
          "default"
        ],
        "summary": "Generate an image and return its url",
        "description": "This API is used to generate an image based on a given query. It then returns the url of that image",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "prompt": "Generate an image about climate change. It should contain a comparison of between healthy and bad environment."
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {"type": "object"},
