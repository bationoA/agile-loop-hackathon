    "/v20.0/331866956681863/feed": {
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Post a text content on a Facebook page",
        "description": "This API is used to post a text only content on a Facebook page. The context is only for text.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "message": "Hello world!",
                  "access_token": "{token}"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {"type": "object"},
                "examples": {
                  "response": {
                    "value": {"id": "{{id}}"}
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v20.0/331866956681863/photos": {
      "post": {
        "tags": [
          "default"
        ],
        "summary": "Post an image on a facebook page",
        "description": "This API is used to post a content containing an image on a Facebook page. The context is only when an image is present in the post.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "url": "https://raw.githubusercontent.com/bationoA/benin-multimodal-ai-hackathon/main/assets/images/output-1.png",
                  "message": "Visit Benin Now!",
                  "access_token": "{token}"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {"id": "122095160744377691", "post_id": "331866956681863_122095160834377691"}
            }
          }
        }
      }
    },
